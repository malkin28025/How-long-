<h1>Add a memorial</h1>

<%= form_for @victim do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br/>
  <%= f.label :email_address %>
  <%= f.text_area :email_address %>
  <br/>
  <%= f.label :phone, 'Phone number' %>
  <%= f.telephone_field :phone %>
  <br/>
  <%= f.submit 'DO IT! DO IT NOW!' %>
<% end %>

</html>